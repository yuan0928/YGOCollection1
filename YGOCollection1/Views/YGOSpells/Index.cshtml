@model IEnumerable<YGOCollection1.Models.YGOSpells>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.CardName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CardPassword)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.create_at)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.update_at)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CardNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.refCardList.code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TypeSpellCard.cardtype)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.CardName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CardPassword)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.create_at)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.update_at)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.CardNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.refCardList.code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.TypeSpellCard.cardtype)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.CardID }) |
            @Html.ActionLink("Details", "Details", new { id=item.CardID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.CardID })
        </td>
    </tr>
}

</table>
